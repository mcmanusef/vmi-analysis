    "domega = np.sin(theta) * dtheta * dphi\n",
    "lmax = 19\n",
    "Y = {\n",
    "    (l, m): scipy.special.sph_harm(m, l, phi, theta)\n",
    "    for l in range(lmax + 1)\n",
    "    for m in range(-l, l + 1)\n",
    "}\n",
    "Y_real = {\n",
    "    (l, m): np.real(\n",
    "        int(m < 0) * np.sqrt(1 / 2) * (Y[(l, -m)] - 1j * Y[(l, m)])\n",
    "        + int(m == 0) * Y[(l, 0)]\n",
    "        + int(m > 0) * np.sqrt(1 / 2) * (Y[(l, m)] + 1j * Y[(l, -m)]) * (-1) ** m\n",
    "    )\n",
